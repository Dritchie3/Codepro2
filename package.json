{
  "name": "codeproconnect",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "connect-mongo": "^2.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "mongoose": "^5.0.2",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "build": "npm run build",
    "api": "nodemon server.js",
    "dev": "concurrently \"cd client && react-scripts start\" \"nodemon server.js\"",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build "
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^1.17.5"
  }
}
